select.bgm = data/SFXWW/sound/select.mp3
select.bgm.volume = 
select.bgm.loop = 1
select.bgm.loopstart =
select.bgm.loopend = 
  ;Music to play at versus screen.
vs.bgm =  data/SFXWW/sound/versus.mp3
vs.bgm.volume = 
vs.bgm.loop = 0
vs.bgm.loopstart =
vs.bgm.loopend = 
  ;Music to play at victory screen.
victory.bgm =  data/SFXWW/sound/victoly.mp3
victory.bgm.volume =
victory.bgm.loop = 0
victory.bgm.loopstart =
victory.bgm.loopend = 

